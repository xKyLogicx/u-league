{
    "version": 2,
    "builds": [
        {
            "src": "u-league/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb",
                "runtime": "python3.8.5"
             }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": { 
                "distDir": "staticfiles_build"
             }
        }
    ],
    "routes": [
        {
            "src": "/templates/(.*)",
            "dest": "/templates/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/u-league/wsgi.py"
        }
    ]
}